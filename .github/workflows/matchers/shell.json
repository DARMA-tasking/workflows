{
    "problemMatcher": [
      {
        "owner": "shell-error",
        "severity": "error",
        "pattern": [
          {
            "regexp": "^[error]:\\s(.+)$"
          }
        ]
      },
      {
        "owner": "shell-warning",
        "severity": "warning",
        "pattern": [
          {
            "regexp": "^[warning]:\\s(.+)$"
          }
        ]
      },
      {
        "owner": "shell-notice",
        "severity": "notice",
        "pattern": [
          {
            "regexp": "^[notice]:\\s(.+)$"
          }
        ]
      }
    ]
  }